{
  "name": "@santander/mfe-app",
  "scripts": {
    "start": "react-env && webpack serve",
    "start:standalone": "webpack serve --env standalone",
    "build": "react-env && concurrently yarn:build:*",
    "build:webpack": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint:ts": "eslint src --ext js,ts,tsx",
    "lint:ts:fix": "eslint . --fix --ext .ts,.tsx",
    "lint:scss": "stylelint **/*.{scss,css}",
    "lint:scss:fix": "stylelint --fix **/*.{scss,css}",
    "lint:json": "prettier --check \"./**/*.json\"",
    "lint:json:fix": "prettier --write \"./**/*.json\"",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "check:all": "yarn prettier:check && yarn lint:ts && yarn lint:scss && yarn lint:json",
    "fix:all": "yarn prettier:format && yarn lint:ts:fix && yarn lint:scss:fix && yarn lint:json:fix",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "build:types": "tsc",
    "doc": "jsdoc -c jsdoc.json",
    "husky:prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.3",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^27.5.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.2.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jsdoc": "^4.0.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "stylelint": "^16.2.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.1.0",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-config-single-spa-react": "^4.0.0",
    "webpack-config-single-spa-react-ts": "^4.0.0",
    "webpack-config-single-spa-ts": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@beam-australia/react-env": "3.0.4",
    "@types/jest": "^29.5.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.2",
    "axios": "^1.6.7",
    "better-docs": "^2.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.70.0",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.3.1"
  },
  "types": "dist/santander-mfe-app.d.ts",
  "version": "1.0.0",
  "main": "index.js",
